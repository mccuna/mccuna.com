{
  "cSpell.words": [
    "clsx",
    "Faunadb",
    "headlessui",
    "Hrefs",
    "miniflare",
    "pnpm",
    "tailwindcss",
    "typecheck"
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts, ${capture}.specs.md, ${capture}.action.ts, ${capture}.loader.ts, ${capture}.meta.ts, ${capture}.error-boundary.tsx, ${capture}.decision.md",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml",
    ".eslintrc": ".eslintrc, .eslintignore, .eslintcache"
  },
  "files.exclude": {
    ".cache": true,
    "node_modules": true
  },
  "search.exclude": {
    "**/pnpm-lock.yaml": true
  }
}
