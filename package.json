{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "pnpm clean && pnpm build:css && pnpm build:remix",
    "build:remix": "remix build",
    "build:css": "tailwindcss -m -i ./app/styles/global.css -o ./app/styles/tailwind.css",
    "clean": "rimraf build public/build",
    "deploy-staging": "pnpm build && wrangler publish --env staging",
    "deploy-prod": "pnpm build && wrangler publish --env production",
    "dev": "pnpm build:remix && pnpm concurrently pnpm:dev:* --kill-others-on-fail",
    "dev:css": "tailwindcss -w -i ./app/styles/global.css -o ./app/styles/tailwind.css",
    "dev:remix": "remix watch",
    "dev:miniflare": "cross-env NODE_ENV=development miniflare ./build/index.js --watch",
    "lint": "eslint --cache . --ext .ts,.tsx,.js,.jsx",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "typecheck": " tsc -b",
    "prepare": "husky install",
    "sync-images-cdn": "node -r esbuild-register ./tools/images/index.ts"
  },
  "dependencies": {
    "@fontsource/abeezee": "^4.5.9",
    "@hcaptcha/react-hcaptcha": "^1.4.4",
    "@headlessui/react": "^1.6.6",
    "@remix-run/cloudflare": "^1.7.0",
    "@remix-run/cloudflare-workers": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "faunadb": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.15.0",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "esbuild": "^0.15.6",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "miniflare": "^2.7.1",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.2",
    "wrangler": "^2.0.28"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@7.9.5",
  "pnpm": {
    "patchedDependencies": {
      "faunadb@4.6.0": "patches/faunadb@4.6.0.patch"
    }
  }
}
