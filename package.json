{
  "private": true,
  "sideEffects": false,
  "main": "./dist/worker.mjs",
  "scripts": {
    "build": "pnpm clean && pnpm build:css && pnpm build:remix && pnpm build:worker",
    "build:remix": "remix build",
    "build:css": "tailwindcss -m -i ./app/styles/global.css -o ./app/styles/tailwind.css",
    "build:worker": "cross-env NODE_ENV=production node ./scripts/build-worker.mjs",
    "clean": "rimraf build public/build dist",
    "deploy-staging": "pnpm build && wrangler publish --env staging",
    "deploy-prod": "pnpm build && wrangler publish --env production",
    "dev": "pnpm build && pnpm concurrently pnpm:dev:* --kill-others-on-fail",
    "dev:css": "tailwindcss -w -i ./app/styles/global.css -o ./app/styles/tailwind.css",
    "dev:remix": "remix watch",
    "dev:miniflare": "miniflare dist/worker.mjs --build-command \"node ./scripts/build-worker.mjs\" --modules --no-cache --watch --open",
    "lint": "eslint --cache . --ext .ts,.tsx,.js,.jsx",
    "start": "cross-env NODE_ENV=production miniflare ./build/index.js",
    "typecheck": " tsc -b",
    "prepare": "husky install",
    "sync-images-cdn": "node -r esbuild-register ./tools/images/index.ts",
    "pw:test": "pnpm build && pnpm playwright test",
    "pw:show-report": "playwright show-report"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.2.0",
    "@fontsource/abeezee": "^4.5.9",
    "@hcaptcha/react-hcaptcha": "^1.4.4",
    "@headlessui/react": "^1.7.2",
    "@remix-run/cloudflare": "^1.7.1",
    "@remix-run/cloudflare-workers": "^1.7.1",
    "@remix-run/react": "^1.7.1",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.2",
    "faunadb": "4.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.16.0",
    "@playwright-testing-library/test": "^4.4.0",
    "@playwright/test": "^1.26.0",
    "@remix-run/dev": "^1.7.1",
    "@remix-run/eslint-config": "^1.7.1",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "autoprefixer": "^10.4.11",
    "concurrently": "^7.4.0",
    "esbuild": "^0.15.7",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "miniflare": "^2.9.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3",
    "undici": "^5.10.0",
    "wrangler": "^2.1.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@7.10.0",
  "pnpm": {
    "patchedDependencies": {
      "faunadb@4.7.0": "patches/faunadb@4.7.0.patch"
    }
  }
}
